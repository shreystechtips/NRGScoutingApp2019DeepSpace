<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="NRGScoutingApp.NavTab"
    xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
    android:TabbedPage.ToolbarPlacement="Bottom"
    android:TabbedPage.BarItemColor="{DynamicResource TabAccentColor}"
    android:TabbedPage.BarSelectedItemColor="{DynamicResource TabMainColor}"
    android:TabbedPage.BarBackgroundColor="{DynamicResource backgroundColor}"
    Title="NRG Scouting"
            BackgroundColor="{DynamicResource backgroundColor}">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Change Event" Order="Secondary" Clicked="changeEventClicked"/>
    </TabbedPage.ToolbarItems>
</TabbedPage>