<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="NRGScoutingApp.MatchEntryEditTab"
    BarBackgroundColor="{DynamicResource backgroundColor}"
            SelectedTabColor="{DynamicResource TabMainColor}"
            BarTextColor="{DynamicResource TabAccentColor}"
    Title="{Binding teamName}">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Back" Clicked="backClicked"/>
    </TabbedPage.ToolbarItems>
</TabbedPage>